    "                plt.xlabel(itemx, fontsize=10)\n",
    "                plt.ylabel(itemy, fontsize=10)\n",
    "                plt.show()\n",
    "                plt.savefig(f\"{output}scatter/{itemx}_{itemy}.png\")\n",
